
<div class="container">
  <div class="row text-center">
    <div class="col-xs-12 col-md-6 col-md-offset-3">
      <% @lines.each do |line| %>
        <h2><%= link_to line.title, line_path(line) %> </h2>
        <p>Location: <%= line.location %></p>
        <p>Start time: <%= line.start_time %></p>
      <% end %>
    </div>
  </div>
</div>
